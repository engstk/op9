		rmnet_shs_main.o \
		rmnet_shs_common.o \
		rmnet_shs_wq.o \
		rmnet_shs_freq.o \
		rmnet_shs_wq_mem.o \
		rmnet_shs_wq_genl.o \
		rmnet_shs_modules.o
